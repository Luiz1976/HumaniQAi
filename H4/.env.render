# Configurações específicas para deploy no Render.com
NODE_ENV=production
PORT=10000

# URLs de produção
FRONTEND_URL=https://www.humaniqai.com.br
CORS_ORIGIN=https://www.humaniqai.com.br

# Configurações de segurança
JWT_SECRET=your_jwt_secret_here

# Database (será configurado automaticamente pelo Render)
