# Configurações opcionais (configurar no painel do Render se necessário)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PUBLIC_KEY=
GOOGLE_API_KEY=
SENDGRID_API_KEY=

# Configurações de logging
LOG_LEVEL=info